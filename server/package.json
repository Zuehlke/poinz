{
  "name": "poinz-server",
  "version": "0.7.3",
  "description": "Distributed Planning Poker",
  "private": true,
  "author": "xeronimus@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Zuehlke/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuehlke/poinz.git"
  },
  "main": "lib/server.js",
  "scripts": {
    "build": "babel src -d lib --source-maps true",
    "test": "jest \"./test/unit\"",
    "test:integration": "jest \"./test/integration\"",
    "docu": "node ./docu/docuGenerator.js",
    "pretest": "npm run lint",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" \"docu/**/*.js\"",
    "start": "node lib/server.js",
    "start:dev": "node src/server.dev.js"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/unit/setup.js"
    ],
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/test/**/*Test.js"
    ]
  },
  "dependencies": {
    "express": "4.17.1",
    "express-sslify": "^1.2.0",
    "glob": "7.1.6",
    "mongodb": "^3.6.2",
    "papaparse": "^5.3.0",
    "redis": "3.0.2",
    "socket.io": "2.3.0",
    "tv4": "1.3.0",
    "uuid": "8.3.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "babel-jest": "^26.3.0",
    "chalk": "^4.1.0",
    "ejs": "^3.1.5",
    "eslint": "7.10.0",
    "jest": "^26.4.2",
    "socket.io-client": "2.3.1"
  },
  "engines": {
    "node": ">=10"
  }
}
